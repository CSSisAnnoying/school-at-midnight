{
    "name": "School At Midnight",
    "version": "0.1",
    "description": "Adds dark mode to google classroom",
    "manifest_version": 3,
    "icons": {
        "16": "/Icons/16x16.png",
        "48": "/Icons/48x48.png",
        "128": "/Icons/128x128.png",
        "256": "/Icons/256x256.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://classroom.google.com/*"
            ],
            "js": [
                "./Renderer/main.js",
                "./Renderer/darkModeStyle.js",
                "./Renderer/themeSwitcher.js"
            ],
            "css": [
                "./Renderer/miscellaneous.css"
            ]
        }
    ],
    "permissions": [
        "storage",
        "activeTab"
    ]
}